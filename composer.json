{
    "name": "jungi/theme-bundle",
    "description": "The friendly themes engine designed for the Symfony2",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Piotr Kugla",
            "email": "piku235@gmail.com"
        }
    ],
    "keywords": [
        "theme",
        "themes",
        "theming",
        "symfony2"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/templating": "~2.3",
        "symfony/twig-bundle": "~2.3",
        "mobiledetect/mobiledetectlib": "~2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "symfony/finder": "~2.3",
        "symfony/validator": "~2.3"
    },
    "suggest": {
        "symfony/finder": "For using the cache warmer",
        "symfony/validator": "For using the ValidationListener"
    },
    "autoload": {
        "psr-4": {
            "Jungi\\Bundle\\ThemeBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}