{
    "name": "jungi/theme-bundle",
    "description": "The friendly themes engine designed for the Symfony2",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Piotr Kugla",
            "email": "piku235@gmail.com"
        }
    ],
    "keywords": [
        "theme",
        "themes",
        "theming",
        "symfony2"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3.9",
        "symfony/framework-bundle": "^2.5|^3.0",
        "symfony/templating": "^2.5|^3.0",
        "symfony/twig-bundle": "^2.5|^3.0",
        "mobiledetect/mobiledetectlib": "^2.5|^3.0"
    },
    "require-dev": {
        "symfony/finder": "^2.5|^3.0",
        "symfony/validator": "^2.5|^3.0"
    },
    "suggest": {
        "symfony/finder": "For using the cache warmer",
        "symfony/validator": "For using the ValidationListener"
    },
    "autoload": {
        "psr-4": {
            "Jungi\\Bundle\\ThemeBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
