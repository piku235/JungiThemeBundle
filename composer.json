{
    "name": "jungi/theme-bundle",
    "description": "A powerful themes engine designed for the symfony2",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Piotr Kugla",
            "email": "piku235@gmail.com"
        }
    ],
    "keywords": [
        "theme",
        "themes",
        "theming",
        "symfony2"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3",
        "symfony/twig-bundle": "~2.3",
        "mobiledetect/mobiledetectlib": "~2.7"
    },
    "suggest": {
        "symfony/form": "If you wanna use the ThemeType",
        "symfony/validator": "For the ValidationListener"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    },
    "autoload": {
        "psr-4": {
            "Jungi\\Bundle\\ThemeBundle\\": ""
        }
    }
}